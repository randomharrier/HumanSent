{
  "name": "humansent-agents",
  "version": "0.1.0",
  "description": "Autonomous agent simulation for testing Precedent AI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "inngest-cli dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "control": "npx ts-node scripts/control.ts",
    "status": "npx ts-node scripts/control.ts status",
    "tick": "npx ts-node scripts/control.ts tick",
    "scenarios": "npx ts-node scripts/control.ts list-scenarios",
    "pause": "npx ts-node scripts/control.ts pause",
    "resume": "npx ts-node scripts/control.ts resume"
  },
  "keywords": [
    "ai-agents",
    "simulation",
    "inngest",
    "typescript"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@slack/web-api": "^7.7.0",
    "@supabase/supabase-js": "^2.47.10",
    "express": "^4.21.2",
    "googleapis": "^144.0.0",
    "inngest": "^3.27.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "inngest-cli": "^1.3.3",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}

